<script type="text/x-template" id="auditing-template">
    <div>
        <div class="table_tou">
            <h6 class="fl">敏感词管理</h6>
            <div class="fr sw_padd" >
            <span class="sw_num"
                  v-cloak >{{totalCount}}</span>&nbsp; &nbsp;<span style="color:#738495">条记录</span>
            <span class="Gradual" @click="shade()">
              <span class="iconfont icon-jiekuanshenqing">&nbsp;新增</span>
            </span>
            <span class="Gradual" @click="importFile()">
            <span class="iconfont icon-export">&nbsp;导出</span>
          </span>
            </div>
        </div>
        <div class="table_nav">
            <form class="layui-form layui-form-pane" action="" >
                <div class="layui-inline" style="margin-top: 10px;">
                    <label class="padR_25">内容分类</label>
                    <div class="layui-input-inline" style="width: auto;">
                        <input type="radio" lay-filter="bizType" name="bizType" value="1" title="营销">
                        <input type="radio" lay-filter="bizType" name="bizType" value="2" title="行业通知">
                        <input type="radio" lay-filter="bizType" name="bizType" value="3" title="验证码">
                        <input type="radio" lay-filter="bizType" name="bizType" value="5" title="五大类（教育、金融、房贷、房产、移民）">
                        <input type="radio" lay-filter="bizType" name="bizType" value="4" title="挂机模版">
                        <input type="radio" lay-filter="bizType" name="bizType" value="55" title="自己输入">
                    </div>
                </div>
                <div class="layui-inline dn conceal">
                    <div class="layui-input-inline">
                        <input
                                type="text"
                                name="param"
                                placeholder="请输入"
                                class="layui-input phone"
                                />
                    </div>
                </div>
                <div class="layui-input-inline" style="position: absolute; right: 20px;bottom: 15px;z-index: 111;">
                    <button class="sw_btn1" lay-submit="" lay-filter="demo2">
                        查询
                    </button>
                </div>
                <br>
                <div class="layui-inline" style="width:49%">
                    <label class="padR_25">接单动作</label>
                    <div class="layui-input-inline" style="width:auto !important">
                        <input  v-cloak
                                type="checkbox"
                                :value="index+1"
                                name="actionType"
                                lay-skin="primary"
                                :title="li"
                                v-for="(li, index) in content1"
                                />
                    </div>
                </div>
                <div class="layui-inline" style="width:49%">
                    <label class="padR_48">状态</label>
                    <div class="layui-input-inline" style="width:auto !important">
                        <input type="radio" name="statusInt" value="1" title="可用" checked>
                        <input type="radio" name="statusInt" value="0" title="不可用" >
                        <input type="radio" name="statusInt" value="2" title="测试">
                    </div>
                </div>
            </form>
            <div class="sw_row">
                <table style="margin-top: 15px;">
                    <tr>
                        <th style="width:108px">表达式ID</th>
                        <th style="width:572px">表达式</th>
                        <th style="width:180px">内容分类</th>
                        <th style="width:192px">接单动作</th>
                        <th style="width:145px">状态</th>
                        <th style="width:127px">使用方式</th>
                        <th style="width:130px;text-align: center;">操作</th>
                    </tr>
                    <tr  v-cloak v-for="(li, index) in allData">
                        <td>{{li.expressionId}}</td>
                        <td>
                            <div :title="li.expression">{{li.expression}}</div>
                        </td>
                        <td>{{li.bizTypeName}}</td>
                        <td>{{li.actionType}}</td>
                        <td>{{li.statusInt}}</td>
                        <td>{{li.useType}}</td>
                        <td style="text-align: center;">
                            <div style="width:100%;text-align: center;">
                                <span class="sw_btn2" @click="amend(li.expressionId)">修改</span>
                                <span class="sw_btn3" @click="del(li.expressionId)">删除</span>
                            </div>
                        </td>
                    </tr>
                </table>
                <div class="sw_pad8" v-show="mapShow == true">暂无数据</div>
            </div>
            <div id="page"></div>

        </div>
</script>
<script type="text/x-template" id="shade-template">
    <div>
        <div class="shade_con">
            <div class="tongji">
                <h3 v-if="code == 0">新增敏感词</h3>
                <h3 v-if="code == 1">修改敏感词</h3>
                <span class="iconfont icon-shanchu" @click="shanchu()"></span>
            </div>
            <div class="temp">
                <form class="layui-form layui-form-pane" action="" style="border: none;">
                    <em style="padding-left: 0px;"> <span style="color:#FE2121;font-weight:600;">*</span>  表达式</em><br>
                    <textarea name="expression"  v-model="info.expression" id="text2" placeholder="请输入"></textarea>
                    <div class="layui-inline" >
                        <label class=""> <span style="color:#FE2121;font-weight:600;">*</span> 内容分类</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="bizType" id="eventLevel">
                                <option value="">请选择</option>
                                <option  v-cloak
                                        v-for="(li, index) in content"
                                        :value="li.bizTypeId"
                                        >{{li.bizTypeName}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline" >
                        <label class=""> <span style="color:#FE2121;font-weight:600;"  >*</span> 接单动作</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="actionType" id="eventLevelTwo">
                                <option value="-1">请选择</option>
                                <option  v-cloak
                                        v-for="(li, index) in content1"
                                        :value="index + 1"
                                        >{{li}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class=""> <span style="color:#FE2121;font-weight:600;">*</span> 账户状态</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="statusInt" id="eventLevelThree" >
                                <option value="-1">请选择</option>
                                <option  v-cloak
                                        v-for="(li, index) in content2"
                                        :value="index"
                                        >{{li}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class=""> <span style="color:#FE2121;font-weight:600;" >*</span> 使用方式</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="useType" id="eventLevelHour">
                                <option value="1">接单</option>
                                <option value="2">补发</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-input-inline">
                        <button class="sw_btn1" style="margin: 37px 0 0 260px;" lay-submit="" lay-filter="demo6" v-if="code == 0">
                            确认新增
                        </button>
                        <span class="sw_btn1" style="margin:37px 0 0 260px;" lay-submit="" lay-filter="demo7" v-if="code == 1">确认修改</span>
                    </div>

                </form>

            </div>
        </div>

    </div>
</script>
