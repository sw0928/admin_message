                        <th style="width:254px;box-sizing: border-box;padding-left:75px;"">操作</th>
                    </tr>
                    <tr  v-for="(li, index) in allData">
                        <td>{{li.ip}}</td>
                        <td>{{li.ip}}</td>
                        <td>{{li.ip}}</td>
                        <td>{{li.ip}}</td>
                        <td>
                            <div :title="li.ip">{{li.ip}}</div>
                        </td>
                        <td>{{li.ip}}</td>
                        <td><div :title="li.ip">{{li.ip}}</div></td>
                        <td v-if="li.id == '10001'" style="color:#9AC938">
                            已通过
                            <span class="success"></span>
                        </td>
                        <td v-if="li.ip == '3'" style="color:#6E6E6E">
                            报备中
                            <span class="sw_report"></span>
                        </td>
                        <td v-if="li.ip == '2'" style="color:#A9A9A9">
                            已拒绝
                            <span class="sw_refuse"></span>
                        </td>
                        <td v-if="li.ip == '0'" style="color:#F8C411">
                            待审核
                            <span class="audit"></span>
                        </td>

                        <td v-if="li.review == '1'">
                            <div style="width:200px;">
                                <span class="sw_btn2"  @click="examine()">查看</span>
                                <span class="sw_btn2"  @click="amend(li.signId)">修改</span>
                                <span class="sw_btn3" @click="del(li.signId,li.review)">删除</span>
                            </div>
                        </td>
                        <td v-else>
                            <div style="width:200px;">
                                <span class="sw_btn2"  @click="examine()">审核</span>
                                <span class="sw_btn2"  @click="amend(li.signId)">修改</span>
                                <span class="sw_btn3" @click="del(li.signId,li.review)">删除</span>
                            </div>
