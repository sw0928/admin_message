<script type="text/x-template" id="shade-template">
    <div>
        <div class="shade_con">
            <div class="tongji">
                <h3 v-if="code == 0">新增敏感词</h3>
                <h3 v-if="code == 1">修改敏感词</h3>
                <span class="iconfont icon-shanchu" @click="shanchu()"></span>
            </div>
            <div class="temp">
                <form class="layui-form layui-form-pane" action="">
                    <em style="padding-left: 0px;"> <span style="color:#FE2121;font-weight:600;">*</span>  表达式</em><br>
                    <textarea name="expression"  v-model="info.expression" id="text2" placeholder="请输入"></textarea>
                    <div class="layui-inline" >
                        <label class=""> <span style="color:#FE2121;font-weight:600;">*</span> 内容分类</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="bizType" id="eventLevel">
                                <option value="">请选择</option>
                                <option  v-cloak
                                         v-for="(li, index) in content"
                                         :value="li.bizTypeId"
                                        >{{li.bizTypeName}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline" >
                        <label class=""> <span style="color:#FE2121;font-weight:600;"  >*</span> 接单动作</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="actionType" id="eventLevelTwo">
                                <option value="-1">请选择</option>
                                <option  v-cloak
                                         v-for="(li, index) in content1"
                                         :value="index + 1"
                                        >{{li}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class=""> <span style="color:#FE2121;font-weight:600;">*</span> 账户状态</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="statusInt" id="eventLevelThree" >
                                <option value="-1">请选择</option>
                                <option  v-cloak
                                         v-for="(li, index) in content2"
                                         :value="index"
                                        >{{li}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class=""> <span style="color:#FE2121;font-weight:600;" >*</span> 使用方式</label><br />
                        <div class="layui-input-inline" style="width:300px">
                            <select name="useType" id="eventLevelHour">
                                <option value="1">接单</option>
                                <option value="2">补发</option>
                            </select>
                        </div>
                    </div>
                    <div class="layui-input-inline">
                        <button class="sw_btn1" style="margin: 62px 0 0 260px;" lay-submit="" lay-filter="demo6" v-if="code == 0">
                            确认新增
                        </button>
                        <span class="sw_btn1" style="margin: 62px 0 0 260px;" lay-submit="" lay-filter="demo7" v-if="code == 1">确认修改</span>
                    </div>

                </form>

            </div>
        </div>

    </div>
</script>
