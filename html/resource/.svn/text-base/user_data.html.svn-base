                            <input type="checkbox" lay-filter="sw_show_B" name="按用户统计" value="2" title="按用户统计" lay-skin="primary">
                            <input type="checkbox" lay-filter="sw_show_C" name="按网关统计" value="3" title="按网关统计" lay-skin="primary">
                        </div>
                    </div>
