                        <td v-if="sw_show_C">{{li.ismgName}}</td>
                        <td >{{li.mtAmount}}</td>
                        <td v-if="!sw_show_C">{{li.submitFailAmount}}</td>
                        <td  >{{li.submitSuccessAmount}}</td>
                        <td  >{{li.mtReceiptAmount}}</td>
                        <td  >{{li.mtReceiptDelivrd}}</td>
                        <td  >{{li.mtADelived}}</td>
                        <td  >{{li.mtZDelived}}</td>
                        <td  >{{li.mtReceiptUndelivrd}}</td>
                        <td  >{{li.mtUnknown}}</td>
                        <td  >{{li.mtUpAmount}}</td>
                    </tr>
                    </tbody>
                </table>
                <div class="sw_pad8" v-show="mapShow == true">暂无数据</div>
            </div>
            <div id="page"></div>
        </div>
