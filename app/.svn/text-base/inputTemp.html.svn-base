<!DOCTYPE html>
<html lang="zh-cn">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=no">
	<title>模板申请</title>
	<link href="css/style.css" rel="stylesheet"/>
	<link href="css/response.css" rel="stylesheet"/>
</head>
<body class="bg">
<div class="content">
	<div class="header">
		<div class="header-nei">
			<a href="nav.html" class="back-up"><i class="iconfont icon-xiangzuo"></i></a>
			<h2 class="header-title">模板申请</h2>
		</div>
	</div>
	<!--head/-->
	<div class="head-height">&nbsp;</div>
	<!--录入校验在style.js  搜order-btn-->
	<div class="login-reg" style="padding-top:16px;">
		<div class="log-reg-list"><input type="tel" class="log-reg-text2 templateId" placeholder="模版ID"/></div>
		<div class="log-reg-list"><input type="text" class="log-reg-text2 loginName" placeholder="申请用户"/></div>
		<div class="log-reg-list"><input type="text" class="log-reg-text2  templateSms" placeholder="模版内容"/></div>
		<div class="log-reg-list" id="interpolation">
			<select>
				<option value="" selected>请选择</option>
				<option value="0">待审核</option>
				<option value="1">已通过</option>
				<option value="2">已拒绝</option>
				<option value="-1">试用中</option>
			</select>
		</div>
		<div class="log-reg-sub">
			<a href="javascript:;" class="log-reg-btn order-btn">查询</a>
		</div>
	</div>
</div>
<!--content/-->
<div class="bg100"></div>
<!--错误提示-->
<div class="tip-errors-bg">
	<div class="tip-tip-box-nei">
		<strong><i class="iconfont icon-shanchu"></i><em class="errors">错误提示</em></strong>
	</div>
	<!--tip-tip-box/-->
</div>
<!--tip-success-bg/-->
<!--正确提示-->
<div class="tip-success-bg">
	<div class="tip-tip-box-nei">
		<strong><i class="iconfont icon-shanchu" style="color:#0F0;"></i><em class="success">正确提示</em></strong>
	</div>
	<!--tip-tip-box/-->
</div>
<!--tip-success-bg/-->
<script src="js/jquery.min.js"></script>
<script src="js/style.js"></script>
<script type="text/javascript">
	$(function(){
		//定单校验
		$(".order-btn").on('click',function() {
			var review = $("#interpolation  option:selected").val();
			var templateId = $(".templateId").val();
			var loginName = $(".loginName").val().trim();
			var templateSms = $(".login-reg .templateSms").val().trim();
			var orderObj = {
				"templateId": templateId,
				"loginName": loginName,
				"offset": 1,
				"pageSize": 10,
				"templateSms": templateSms,
				"review": review
			};
			sessionStorage.setItem('inputObj', JSON.stringify(orderObj));
			location.href = "template.html"
		})
	})

</script>
</body>
</html>