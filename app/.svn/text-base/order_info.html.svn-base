<!DOCTYPE html>
<html lang="zh-cn">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=no">
<title>订单详情</title>
<link href="css/style.css" rel="stylesheet"/>
<link href="css/response.css" rel="stylesheet"/>
</head>
<body class="bg">
<div class="content">
	<div class="header">
		<div class="header-nei">
			<a href="order_fail.html" class="back-up"><i class="iconfont icon-xiangzuo"></i></a>
			<h2 class="header-title">订单详情</h2>
            <a href="nav.html" class="back-top"><i class="iconfont icon-daohang"></i></a>
		</div>
	</div>
	<!--head/-->
	<div class="head-height">&nbsp;</div>
	<div class="order-info" id="info">

    </div>
</div>
<!--content/-->
<div class="bg100"></div>
<script src="js/jquery.min.js"></script>
<script src="js/style.js"></script>
<script src="js/doT.min.js"></script>
<script id="infotmpl" type="text/x-dot-template">
 <table>
    <tr>
     <th>用户名</th>
     <td>{{=it.loginName}}</td>
    </tr>
    <tr>
     <th>下单时间</th>
     <td>{{=it.createTime}}</td>
    </tr>
    <tr>
     <th>下单来源</th>
     <td>{{=it.submitFrom}}</td>
    </tr>
    <tr>
     <th>内容</th>
     <td>{{=it.msgContent}}</td>
    </tr>
    <tr>
     <th>提交总量</th>
     <td>{{=it.submitMtTotal}}</td>
    </tr>
    <tr>
     <th>号码</th>
     <td>{{=it.msisdn}}</td>
    </tr>
    <tr>
     <th>错误代码</th>
     <td>{{=it.errorCode}}</td>
    </tr>
    <tr>
     <th>错误信息</th>
     <td>{{=it.errorMsg}}</td>
    </tr>
 </table>
</script>
<script>
  $(function(){
   var info = JSON.parse(sessionStorage.getItem('infoObj'));
      console.log(info)
   var interText = doT.template($("#infotmpl").text());
   $("#info").html(interText(info));
  })
</script>
</body>
</html>